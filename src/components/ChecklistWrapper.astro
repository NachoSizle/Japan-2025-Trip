---
import ChecklistSolid from "./ChecklistSolid";
import { getCollection, type CollectionEntry } from 'astro:content';

const checklist = await getCollection("checklist");
type ChecklistEntry = CollectionEntry<'checklist'>['data'];
---

<ChecklistSolid checklist={checklist.map((entry: CollectionEntry<'checklist'>): ChecklistEntry => entry.data)} client:visible />
